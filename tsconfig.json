{
    "compilerOptions": {
        "target": "ES2023",
        "module": "esnext",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowArbitraryExtensions": true,
    },
    // "include": ["src/**/*", "frontend_app/src/**/*"]
   
}